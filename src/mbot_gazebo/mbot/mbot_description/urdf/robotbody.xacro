    </collision>
  </link>
  
  <joint name="Joint_Wheel_Front_Right" type="continuous">
    <origin xyz="0.22383 -0.2459 0.047662" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="Right_Front_Wheel" />
    <axis xyz="0 1 0" />
  </joint>
  
  
